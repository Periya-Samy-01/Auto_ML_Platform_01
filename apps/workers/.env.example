# Worker Environment Configuration
# Copy this file to .env and update values

# Database (must match API database)
DATABASE_URL=postgresql://user:password@host:5432/automl_dev

# Redis (for Celery broker and cache)
REDIS_URL=redis://localhost:6379/0

# Cloudflare R2 Storage
R2_ACCOUNT_ID=your-account-id
R2_ACCESS_KEY_ID=your-access-key
R2_SECRET_ACCESS_KEY=your-secret-key
R2_BUCKET_NAME=automl-datasets-production

# Optional: Override endpoint URL (normally constructed from account ID)
# R2_ENDPOINT_URL=https://your-account-id.r2.cloudflarestorage.com

# Worker Configuration
CELERY_LOG_LEVEL=info
WORKER_CONCURRENCY=2
WORKER_MAX_TASKS_PER_CHILD=10

# Processing Limits
MAX_FILE_SIZE_MB=1000
PROCESSING_TIMEOUT_MINUTES=30
